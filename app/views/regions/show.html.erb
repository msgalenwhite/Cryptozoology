<h1 class="index-title"> <%= @region.name %> </h1>

<div class="row">
  <div class="small-11 medium-5 columns link-container">
    <h3 class="cryptids-title">Cryptids</h3>
  <ul class="cryptid-links">
    <% @cryptids.each do |cryptid| %>
    <div class="image-line-height">
       <img class="dino-bullet" src="data:image/png;base64,"/>
      <%= link_to cryptid.name, cryptid, :class => "one-cryptid-link" %>
    </div>
    <% end %>
  </ul>
  </div>

  <div class="small-11 medium-5 columns highlight-container">
    <h3 class="spotted-title">Recently Spotted</h3>
      <div id="app">
      </div>
  </div>
</div>
